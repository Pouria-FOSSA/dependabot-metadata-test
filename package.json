    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@octokit/rest": "^21.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.22",
    "concurrently": "^8.2.2",
    "parcel": "^2.12.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
